<%= semantic_form_for @yemek_uretim do |f| %>
  <%= f.inputs do %>
    <%= f.input :yemek_id %>
    <%= f.input :mekan_id %>
    <%= f.input :menu_id %>
    <%= f.input :miktar %>
    <%= f.input :birim_id %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
