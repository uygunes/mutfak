<%= semantic_form_for @stok_takip do |f| %>
  <%= f.inputs do %>
    <%= f.input :menu_id %>
    <%= f.input :mekan_id %>
    <%= f.input :siparis_formu_id %>
    <%= f.input :teslimat_formu_id %>
    <%= f.input :malzeme_id %>
    <%= f.input :user_id %>
    <%= f.input :miktar %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
